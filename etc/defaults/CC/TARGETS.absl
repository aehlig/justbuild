{ "defaults":
  { "type": ["CC", "defaults"]
  , "base": [["@", "base", "CC", "defaults"]]
  , "ADD_CXXFLAGS":
    [ "-std=c++20"
    , "-Wall"
    , "-Wextra"
    , "-Wcast-qual"
    , "-Wconversion-null"
    , "-Wformat-security"
    , "-Wmissing-declarations"
    , "-Woverlength-strings"
    , "-Wpointer-arith"
    , "-Wundef"
    , "-Wunused-local-typedefs"
    , "-Wunused-result"
    , "-Wvarargs"
    , "-Wvla"
    , "-Wwrite-strings"
    , "-DNOMINMAX"
    ]
  }
}
