{ "defaults":
  { "type": ["CC", "defaults"]
  , "base": [["@", "base", "CC", "defaults"]]
  , "ADD_CFLAGS": ["-std=gnu17"]
  , "ADD_CXXFLAGS":
    [ "-std=c++20"
    , "-DHAVE_PTHREAD"
    , "-DHAVE_ZLIB"
    , "-Woverloaded-virtual"
    , "-Wno-sign-compare"
    , "-Wno-unused-function"
    , "-Wno-write-strings"
    , "-Wno-deprecated-declarations"
    ]
  }
}
