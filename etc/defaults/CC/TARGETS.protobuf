{ "defaults":
  { "type": ["CC", "defaults"]
  , "CC": ["clang"]
  , "CXX": ["clang++"]
  , "CFLAGS": ["-std=gnu17", "-O2"]
  , "CXXFLAGS":
    [ "-std=c++20"
    , "-O2"
    , "-DHAVE_PTHREAD"
    , "-DHAVE_ZLIB"
    , "-Woverloaded-virtual"
    , "-Wno-sign-compare"
    , "-Wno-unused-function"
    , "-Wno-write-strings"
    , "-Wno-deprecated-declarations"
    ]
  , "AR": ["ar"]
  , "PATH": ["/bin", "/sbin", "/usr/bin", "/usr/sbin"]
  }
}
